{
  "name": "rcaron",
  "description": "RCaron(Ř) for VSCode",
  "displayName": "RCaron",
  "author": "Jan0660",
  "license": "MIT",
  "version": "0.0.1",
  "publisher": "Jan0660",
  "repository": "https://github.com/Jan0660/rcaron-vscode",
  "engines": {
    "vscode": "^1.71.2"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:rcaron"
  ],
  "main": "./out/src/extension",
  "enableProposedApi": true,
  "contributes": {
    "configurationDefaults": {
      "[rcaron]": {
        "editor.semanticHighlighting.enabled": true
      }
    },
    "languages": [
      {
        "id": "rcaron",
        "extensions": [
          ".rcaron",
          ".ř"
        ],
        "aliases": [
          "RCaron",
          "rcaron",
          "ř",
          "Ř"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "dark": "./icon.png",
          "light": "./icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "rcaron",
        "scopeName": "source.rcaron",
        "path": "./syntaxes/rcaron.tmLanguage.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "RCaron",
      "properties": {
        "rcaron.languageServerPath": {
          "type": "string",
          "default": null,
          "markdownDescription": "%rcaron.configuration.languageServerPath.description%"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
		"clean": "gulp clean"
  },
  "devDependencies": {
    "@types/mocha": "8.0.0",
    "@types/node": "14.0.25",
    "typescript": "4.8.4",
    "@types/vscode": "1.71.0",
    "vscode-nls-dev": "^4.0.0"
  },
  "dependencies": {
    "vscode-languageclient": "^7.0.0-next.8",
    "vscode-languageserver-protocol": "^3.16.0-next.6",
    "vscode-nls": "^5.0.0"
  }
}
